<e2:screen external_module="Plugins.Extensions.InternetRadio.WebScreens.InternetRadioWebScreens" name="InternetRadioWebScreens">&lt;?xml version="1.0" encoding="UTF-8"?>
<e2internetradio_favoritelist>
<e2:element source="InternetRadioFavoriteList">				
	<e2:convert type="web:ListFiller">
	&lt;e2internetradio_favorite_item>
		&lt;e2favoritename><e2:item name="Name" filter="xml" />&lt;/e2favoritename>
		&lt;e2favoritetext><e2:item name="Text" filter="xml" />&lt;/e2favoritetext>
		&lt;e2favoritetype><e2:item name="Type" filter="xml" />&lt;/e2favoritetype>
		&lt;e2favoritetags><e2:item name="Tags" filter="xml" />&lt;/e2favoritetags>
		&lt;e2favoritecountry><e2:item name="Country" filter="xml" />&lt;/e2favoritecountry>
		&lt;e2favoritehomepage><e2:item name="Homepage" filter="xml" />&lt;/e2favoritehomepage>
	&lt;/e2internetradio_favorite_item>
	</e2:convert>
	<e2:convert type="web:TextToHTML" />			
</e2:element>
</e2internetradio_favoritelist>	
</e2:screen>